{
    "name": "69-shades-of-love",
    "version": "1.1.0",
    "description": "Self-care task tracker with 69 tasks to improve wellbeing",
    "author": "WTQ - What The Quack",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore",
        "format": "prettier --write \"src/**/*.{js,ts,vue}\"",
        "typecheck": "vue-tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "pinia": "^2.1.7",
        "vue": "^3.4.0",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.3.2",
        "@types/node": "^20.8.2",
        "@vitejs/plugin-vue": "^5.0.0",
        "@vitest/coverage-v8": "^3.0.9",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.0",
        "eslint": "^8.50.0",
        "eslint-plugin-vue": "^9.15.1",
        "jsdom": "^22.1.0",
        "prettier": "^3.0.0",
        "rollup-plugin-visualizer": "^5.9.0",
        "typescript": "~5.2.0",
        "vite": "^6.2.2",
        "vitest": "^3.0.9",
        "vue-tsc": "^2.2.8"
    }
}